#include <stdio.h>
#include <stdlib.h>
#include <limits.h>

#define MAX_SIZE 1000

typedef struct {
    int arr[MAX_SIZE];
    int min[MAX_SIZE];
    int max[MAX_SIZE];
    int top;
} CustomStack;

void init(CustomStack* stack) {
    stack->top = -1;
}

void push(CustomStack* stack, int x) {
    if (stack->top == MAX_SIZE - 1) {
        printf("Stack Overflow\n");
        return;
    }

    stack->arr[++stack->top] = x;
    if (stack->top == 0) {
        stack->min[stack->top] = x;
        stack->max[stack->top] = x;
    } else {
        stack->min[stack->top] = (x < stack->min[stack->top - 1]) ? x : stack->min[stack->top - 1];
        stack->max[stack->top] = (x > stack->max[stack->top - 1]) ? x : stack->max[stack->top - 1];
    }
}

void pop(CustomStack* stack) {
    if (stack->top == -1) {
        printf("Stack Underflow\n");
        return;
    }
    stack->top--;
}

int top(CustomStack* stack) {
    return (stack->top == -1) ? INT_MIN : stack->arr[stack->top];
}

int getMin(CustomStack* stack) {
    return (stack->top == -1) ? INT_MIN : stack->min[stack->top];
}

int getMax(CustomStack* stack) {
    return (stack->top == -1) ? INT_MAX : stack->max[stack->top];
}

int main() {
    CustomStack stack;
    init(&stack);

    push(&stack, 3);
    push(&stack, 5);
    push(&stack, 2);
    push(&stack, 1);

    printf("Top: %d\n", top(&stack));
    printf("Min: %d\n", getMin(&stack));
    printf("Max: %d\n", getMax(&stack));

    pop(&stack);
    printf("After popping, Min: %d\n", getMin(&stack));

    return 0;
}

